<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Learn_Coding" x:Class="CodeOutputScreen" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="800" Height="600">
    <Grid>
        <local:WebBrowserScreen x:Name="CWebBrowserScreen" Margin="0" Width="Auto" Height="Auto" IsEnabled="False" Visibility="Hidden"/>
        <local:CompilingScreen x:Name="CCompileScreen" Margin="0" Width="Auto" Height="Auto" IsEnabled="False" Visibility="Hidden"/>
        <local:ConsoleScreen x:Name="CConsoleScreen" Margin="0" Width="Auto" Height="Auto" Visibility="Hidden" IsEnabled="False"/>
    </Grid>
</UserControl>
